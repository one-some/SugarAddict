{
  "manifest_version": 2,
  "name": "SugarAddict",
  "version": "0.2",
  "description": "Story game manipulation tool",

  "icons": {
    "48": "logo.png"
  },

  "browser_action": {
    "default_icon": {
      "48": "logo.png"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["*://*/*", "file://*/*"],
      "js": ["main.js"]
    }
  ]
}
